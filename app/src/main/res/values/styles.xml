<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/trans_blue</item>
        <item name="colorPrimaryDark">@color/trans_blue</item>
        <item name="colorAccent">@color/trans_black</item>
    </style>
    <style name="DialogFullScreen" parent="Theme.AppCompat.Light.DialogWhenLarge">
        <item name="android:windowNoTitle">true</item>
        <item name="android:statusBarColor">@color/trans_blue</item>
        <item name="colorPrimary">@color/trans_blue</item>
        <item name="colorPrimaryDark">@color/trans_blue</item>
        <item name="colorAccent">@color/trans_blue</item>
    </style>

</resources>
